{
  "branches": ["develop"],
  "plugins": {
    "commit-analyzer": {
      "name": "default@^1.0.0"
    },
    "changelog-generator": {
      "name": "default",
      "options": {
        "emojis": "true"
      }
    },
    "provider": {
      "name": "github",
      "options": {
        "releases": true
      }
    }
  }
}
